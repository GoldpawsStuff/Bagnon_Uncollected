package-as: Bagnon_Uncollected
enable-nolib-creation: no

externals:
    Bagnon_Uncollected/Libs/TaintLess:
        url: https://www.townlong-yak.com/addons.git/taintless
        commit: default

move-folders:
    Bagnon_Uncollected/Bagnon_Uncollected: Bagnon_Uncollected

manual-changelog:
    filename: CHANGELOG.md
    markup-type: markdown

license-output: LICENSE.txt

ignore:
    - CHANGELOG.md
    - CODE_OF_CONDUCT.md
    - LICENSE.md
    - README.md
